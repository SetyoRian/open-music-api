{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "no-console": "off",
        "object-curly-newline": "off"
    }
}